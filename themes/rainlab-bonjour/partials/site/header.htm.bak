[viewBag]
==
<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">

        <title>{{ this.page.title }} | Frisian Flag Indonesia</title>

        <meta name="title" content="{{ this.page.meta_title}}">
        <meta name="description" content="{{ this.page.meta_description }}">
        <meta name="keywords" content="{{ this.page.meta_keywords }}">

        <meta name="author" content="Frisian Flag Indonesia">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
        <!-- Disable Search Engine Tracking -->
        <!-- <meta name="robots" content="no index, no follow" />
        <meta name="google-site-verification" content="KH51PLY8sFtryG58f7NtBlGGvEWS8PJ_qz_XECzWW2I" /> -->

        <meta property="og:url"           content="http://frisianflag.sudahdistaging.in/aktivitas-kami/nikmatcokelatnya" />
        <meta property="og:type"          content="website" />
        <meta property="og:title"         content="NikmatCokelatnya" />
        <meta property="og:description"   content="Kembalikan semangat keluarga dengan menikmati berbagai macam padu padan Frisian Flag kental manis cokelat. Yuk cobain !" />
        <meta property="og:image"         content="http://frisianflag.sudahdistaging.in/themes/rainlab-bonjour/assets/img/share-facebook.jpg" />


        {% if this.page.fileName == 'home.htm' %}
            <link rel="canonical" href="{{ 'home' | page }}">
        {% elseif this.page.fileName == 'news-archive.htm' %}
            <link rel="canonical" href="{{ 'home' | page }}/perusahaan-kami/berita/year/{{ this.param.slug }}">
        {% elseif this.page.fileName == 'press-release-archive.htm' %}
            <link rel="canonical" href="{{ 'home' | page }}/perusahaan-kami/siaran-pers-archive/year/{{ this.param.slug }}">
        {% else %}
            <link rel="canonical" href="{{ this.page.id | page }}">
        {% endif %}

        <!-- FAVICONS -->
        <link rel="apple-touch-icon" sizes="180x180" href="{{ 'assets/favicon/apple-touch-icon.png'|theme }}">
        <link rel="icon" type="image/png" href="{{ 'assets/favicon/favicon-32x32.png'|theme }}" sizes="32x32">
        <link rel="icon" type="image/png" href="{{ 'assets/favicon/favicon-16x16.png'|theme }}" sizes="16x16">
        <link rel="manifest" href="{{ 'assets/favicon/manifest.json'|theme }}">
        <link rel="mask-icon" href="{{ 'assets/favicon/safari-pinned-tab.svg'|theme }}" color="#5bbad5">
        <meta name="theme-color" content="#ffffff">

        <link rel="stylesheet" href="{{ 'assets/libs/cookie/cookieconsent.min.css'|theme }}"/>

        <!-- STYLESHEET -->
        <link rel="stylesheet" href="{{ 'assets/css/app.css'|theme }}">

        <script src="{{ 'assets/libs/cookie/cookieconsent.min.js'|theme }}" async></script>

        <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5aa749b02758be27" async></script>

        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.0/moment.js"></script>

        <script>
            window.addEventListener("load", function(){
            window.cookieconsent.initialise({
              "palette": {
                "popup": {
                  "background": "#0f4d9a"
                },
                "button": {
                  "background": "#009fe3",
                  "color" : "#ffffff"
                }
              },
              "content": {
                "message": "We use cookies on this page ",
                "dismiss": "Close",
                "link": "What are cookies",
                "href": "{{ 'home' | page }}/kebijakan-cookie"
              }
            })});

            var home = "{{ 'home' | page }}";
        </script>

