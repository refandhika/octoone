        <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
        new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
        j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
        'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
        })(window,document,'script','dataLayer','GTM-PXZ46KJ');</script>
        <!-- End Google Tag Manager -->


    </head>
    <body>

      <div id="fb-root"></div>
      <script>(function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.3";
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
      </script>

    {% placeholder footer %}

    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PXZ46KJ"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->

    <header class="header">
        <!-- desktop menu -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-2 col-xs-4">
                    <a href="{{ 'home'|page }}" class="logo">
                        <div class="logo__default">
                            <img src="{{ 'assets/img/logo.png'|theme }}" srcset="{{ 'assets/img/logo2x.png'|theme }} 2x" alt="Frisian Flag"/>
                             <span>BUILDING<br/>STRONG FAMILIES<br/>SINCE 1871</span>
                        </div>
                        <div class="logo__small">
                            <img src="{{ 'assets/img/logo-small.png'|theme }}" srcset="{{ 'assets/img/logo-small2x.png'|theme }} 2x" alt="Frisian Flag">
                            <span>BUILDING<br/>STRONG FAMILIES<br/>SINCE 1871</span>
                        </div>

                    </a>
                </div>

                <div class="col-md-10 navigation hidden-xs hidden-sm hidden-md">
                    <ul class="top-menu">
                        <li class="top-menu__list">
                            <a href="{{ 'contact-us'|page }}">{{'hubungi-kami'|trans}}</a>
                        </li>
                        <li class="top-menu__list">
                            <a href="{{ url('/')}}/id" class="lang-picker" data-lang="id">
                                <img src="{{ 'assets/img/id.png'|theme }}" srcset="{{ 'assets/img/id2x.png'|theme }} 2x" alt="Indonesian">
                            </a>
                        </li>
                        <li class="top-menu__list">
                            <a href="{{ url('/')}}/en" class="lang-picker" data-lang="en">
                                <img src="{{ 'assets/img/uk.png'|theme }}" srcset="{{ 'assets/img/uk2x.png'|theme }} 2x" alt="English">
                            </a>
                        </li>
                       <li class="top-menu__list top-menu__list--button"><a href="javascript:;" class="searchbtn"><i class="fa fa-search" aria-hidden="true"></i></a></li>
                    </ul>

                    {% if str_contains(url_current(), '/en') %} 
                          {% partial 'site/nav-en' %}
                    {% else %}
                        {% partial 'site/nav' %}
                    {% endif %}

                </div>
                <div class="col-xs-8 col-sm-8 pull-right">
                    <div class="bar-mobile">
                        <div class="wrp-navbar">
                            <span></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end of desktop menu -->

        <!-- Mobile Menu -->
        <div class="headermenu-xs">
            <div class="wrp-headmenu-xs">

                {% if str_contains(url_current(), '/en') %} 
                       
                {% else %}
                    {% partial 'site/mobilemenu' %}
                {% endif %}

                <div class="bottom">
                    <ul>
                        <li><a href="{{ 'contact-us'|page }}"><img src="{{ 'assets/img/contact-us.png'|theme }}" alt="Hubungi Kami"/><span>{{'hubungi-kami'|trans|uppercase}}</span></a></li>
                        <li><a href="javascript:;" class="searchbtn"><img src="{{ 'assets/img/search.png'|theme }}" alt="Search"/><span>SEARCH</span></a></li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- end of mobile menu -->

        <!-- search form -->
        <div class="searchform">
          <form action="{{ 'home'|page }}/pencarian" method="get">
            <input name="q" id="search-result" type="search" placeholder="Masukkan kata kunci dan tekan enter">
            <a class="closesearch"><i class="fa fa-close"></i> </a>
          </form>
        </div>
    </header>